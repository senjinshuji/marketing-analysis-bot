{
  "systemOverview": {
    "name": "マーケティング戦略分析Bot",
    "description": "URLを入力すると、詳細な市場分析を行い、最適な広告媒体と獲得手法を提案するシステム"
  },
  "analysisStructure": {
    "phase1_comprehensiveAnalysis": {
      "description": "URLから包括的な情報を収集・分析",
      "sections": {
        "1_productInfo": {
          "title": "商品・製品情報",
          "items": [
            "商品名",
            "商品URL",
            "カテゴリー",
            "サイズ・容量",
            "機能（箇条書きで複数）",
            "効果（箇条書きで複数）",
            "RTB（Reason to Believe）",
            "権威性"
          ]
        },
        "2_pricing": {
          "title": "価格・販売情報",
          "items": [
            "通常価格",
            "特別オファー価格",
            "キャンペーン情報",
            "発売日",
            "販売チャネル"
          ]
        },
        "3_demographics": {
          "title": "デモグラフィック分析",
          "items": [
            "年齢層分布",
            "性別比率",
            "その他特性（世帯構成、職業等）"
          ]
        },
        "4_valueProposition": {
          "title": "提供価値分析",
          "items": {
            "functionalValue": [
              "時短・効率化価値",
              "問題解決価値",
              "利便性価値"
            ],
            "emotionalValue": [
              "誰が（ペルソナ）",
              "どんな状況で",
              "どう感じるか"
            ]
          }
        },
        "5_marketAnalysis": {
          "title": "市場分析",
          "items": [
            "市場定義（機能・情緒価値が刺さる最大市場）",
            "市場カテゴリー",
            "市場規模（金額）",
            "購入者数（年間）",
            "AOV（平均購入単価）",
            "戦略ターゲット",
            "コアターゲット"
          ]
        },
        "6_n1PersonaAnalysis": {
          "title": "N1ペルソナ詳細分析",
          "profile": {
            "basic": [
              "年齢",
              "性別",
              "居住地",
              "職業",
              "役職",
              "業種",
              "年収",
              "学歴"
            ],
            "lifestyle": [
              "家族構成",
              "生活リズム（平日・休日）",
              "趣味・興味関心",
              "利用メディア",
              "日常の購入商品",
              "非日常の購入商品"
            ]
          },
          "customerJourney": {
            "situation": "具体的なシーン描写",
            "sensory": {
              "external": "五感情報（音、視覚等）",
              "internal": "体感覚（疲労、空腹等）"
            },
            "psychology": {
              "instinct": "本能（生存、子孫繁栄等）",
              "perception": "知覚・バックグラウンド",
              "emotion": "感情（快・不快）",
              "desire": "欲求"
            },
            "demand": {
              "needs": "具体的な需要",
              "category": "求めるカテゴリー",
              "priceRange": "予算感",
              "conditions": "その他必要条件"
            }
          }
        },
        "7_competitorAnalysis": {
          "title": "競合分析",
          "items": {
            "categoryCompetitors": {
              "name": "カテゴリー競合",
              "brands": "競合ブランド名",
              "products": "競合商品・サービス"
            },
            "differentiation": {
              "usp": "独自の便益",
              "preference": "選ばれる理由"
            }
          }
        },
        "8_brandConcept": {
          "title": "ブランドコンセプト",
          "items": [
            "市場での立ち位置",
            "需要の捉え方",
            "特徴（便益提供特徴）",
            "提供便益",
            "コンセプト（一言で表現）",
            "ブランドキャラクター"
          ]
        }
      }
    },
    "phase2_classification": {
      "description": "分析結果を基に案件を分類",
      "step1": {
        "name": "市場タイプ判定",
        "logic": {
          "niche": {
            "criteria": [
              "特定の悩み・課題に特化",
              "専門性が高い",
              "ターゲットが限定的",
              "単価が高い傾向"
            ],
            "examples": ["塾", "専門ペット用品", "B2B向けサービス", "人材", "不動産査定"]
          },
          "mass": {
            "criteria": [
              "幅広い層が対象",
              "一般的な悩み・欲求",
              "認知度重視",
              "価格訴求が効く"
            ],
            "examples": ["DIO", "メンズクリア", "シボラナイト2", "オイグルト", "cocone", "りそこ"]
          }
        }
      },
      "step2": {
        "name": "行動理由分類",
        "categories": {
          "niche_personal": {
            "type": "ニッチ",
            "reason": "自分ごと化させて行動してもらう",
            "targetExamples": "塾、ペット",
            "mediaIds": ["1", "2", "7", "19", "22", "23", "24", "25", "28", "29", "30", "31", "33"]
          },
          "niche_b2b": {
            "type": "ニッチ",
            "reason": "to B向けに適した配信",
            "targetExamples": "人材、不動産査定",
            "mediaIds": ["1", "2", "3", "4", "5", "6", "7", "25", "28", "29", "30", "31"]
          },
          "mass_offer": {
            "type": "マス向け",
            "reason": "オファーが魅力的、とにかく安い（無料、500円オファー、LINE追加など）",
            "targetExamples": "DIO, メンズクリア",
            "mediaIds": ["7", "10", "11", "13", "16", "17", "19", "21", "22", "24", "25", "27", "28", "30", "31"]
          },
          "mass_authority": {
            "type": "マス向け",
            "reason": "訴求が強い、権威性がある、悩みが解決できる（治る）",
            "targetExamples": "シボラナイト2",
            "mediaIds": ["7", "8", "11", "12", "13", "14", "17", "18", "19", "22", "23", "24", "25", "28", "29", "31"]
          },
          "mass_newFact": {
            "type": "マス向け",
            "reason": "新事実系、テクスチャーが特徴的",
            "targetExamples": "オイグルト、cocone、りそこ",
            "mediaIds": ["7", "25", "32", "33"]
          }
        }
      }
    },
